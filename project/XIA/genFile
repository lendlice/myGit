dd if=/dev/urandom of=16kb_b.txt count=16 bs=1024
dd if=/dev/urandom of=17kb_a.txt count=17 bs=1024
dd if=/dev/urandom of=17kb_b.txt count=17 bs=1024
dd if=/dev/urandom of=18kb_a.txt count=18 bs=1024
dd if=/dev/urandom of=18kb_b.txt count=18 bs=1024
dd if=/dev/urandom of=19kb_a.txt count=19 bs=1024
dd if=/dev/urandom of=19kb_b.txt count=19 bs=1024
dd if=/dev/urandom of=20kb_a.txt count=20 bs=1024
dd if=/dev/urandom of=20kb_b.txt count=20 bs=1024
dd if=/dev/urandom of=21kb_a.txt count=21 bs=1024
dd if=/dev/urandom of=21kb_b.txt count=21 bs=1024
dd if=/dev/urandom of=22kb_a.txt count=22 bs=1024
dd if=/dev/urandom of=22kb_b.txt count=22 bs=1024
